{
  "name": "chait",
  "displayName": "Chait",
  "description": "VS Code extension with a custom bottom panel tab",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [],
  "contributes": {
    "viewsContainers": {
      "panel": [
        {
          "id": "myPanel",
          "title": "Chait",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "myPanel": [
        {
          "type": "webview",
          "id": "myPanel.view",
          "name": "My Custom Tab",
          "icon": "media/icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "chait.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "build": "node esbuild.js",
    "watch": "node esbuild.js --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "package": "npm run check-types && npm run lint && node esbuild.js --production"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/vscode": "^1.108.1",
    "@types/ws": "^8.18.1",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "glob": "^13.0.0",
    "mocha": "^11.7.5",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "ws": "^8.19.0"
  }
}
